{
  "name": "@jengaui/hooks",
  "version": "0.1.3",
  "license": "MIT",
  "description": "Jenga UI Hooks component in React",
  "keywords": [
    "react",
    "hooks",
    "Jenga UI",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "tastycss",
    "library",
    "design-system"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@jengaui/utils": "^0.1.3"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
