{
  "name": "@jenga-ui/button",
  "version": "0.0.1",
  "description": "Jenga-UI React button component",
  "keywords": [
    "button",
    "react",
    "jenga-ui",
    "ui",
    "clickable",
    "tabbable"
  ],
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prebuild": "rimraf dist && rimraf types",
    "postbuild": "yarn build:types",
    "build:types": "npx tsc -b --verbose"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@react-types/button": "^3.4.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "tastycss-react": "^0.4.4"
  }
}
