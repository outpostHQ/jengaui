{
  "name": "@jenga-ui/placeholder",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Jenga-UI React placeholder component",
  "keywords": [
    "placeholder",
    "react",
    "jenga-ui",
    "ui"
  ],
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prebuild": "rimraf dist && rimraf types",
    "postbuild": "yarn build:types",
    "build:types": "npx tsc -b --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.13"
  },
  "dependencies": {
    "tastycss": "^0.7.5"
  },
  "homepage": "https://github.com/numldesign/jenga-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numldesign/jenga-ui.git"
  },
  "bugs": {
    "url": "https://github.com/numldesign/jenga-ui/issues"
  }
}
