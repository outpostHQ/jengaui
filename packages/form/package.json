{
  "name": "@jenga-ui/form",
  "version": "0.1.1",
  "description": "Jenga-UI React form component",
  "keywords": [
    "form",
    "react",
    "jenga-ui",
    "ui",
    "clickable",
    "tabbable"
  ],
  "repository": "OutpostHQ/jenga-ui",
  "license": "MIT",
  "bugs": "https://github.com/OutpostHQ/jenga-ui/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@jenga-ui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jenga-ui/content": "*",
    "@jenga-ui/layout": "*",
    "@jenga-ui/providers": "*",
    "@jenga-ui/tooltip": "*",
    "@jenga-ui/utils": "*",
    "@react-spectrum/utils": "^3.6.3",
    "@react-stately/collections": "^3.3.4",
    "@react-types/shared": "^3.10.1",
    "email-validator": "^2.0.4",
    "styled-components": "^5.3.0",
    "tastycss": "^0.8.6",
    "valid-url": "^1.0.9"
  }
}
