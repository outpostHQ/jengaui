{
  "name": "@jenga-ui/new-notifications",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Responsive and accessible React UI components built with TastyCSS",
  "keywords": [
    "react",
    "jenga-ui",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "tastycss",
    "library",
    "design-system"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@jenga-ui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jenga-ui/button": "*",
    "@jenga-ui/button-group": "*",
    "@jenga-ui/content": "*",
    "@jenga-ui/dialog": "*",
    "@jenga-ui/hooks": "*",
    "@jenga-ui/layout": "*",
    "@jenga-ui/portal": "*",
    "@jenga-ui/utils": "*",
    "@react-aria/button": "^3.6.1",
    "@react-aria/focus": "^3.8.0",
    "@react-aria/interactions": "^3.11.0",
    "@react-aria/selection": "^3.10.1",
    "@react-aria/utils": "^3.13.3",
    "@react-aria/visually-hidden": "^3.4.1",
    "@react-spectrum/utils": "^3.7.3",
    "@react-stately/collections": "^3.4.3",
    "@react-stately/list": "^3.5.3",
    "@react-types/shared": "^3.14.1",
    "react-is": "^18.2.0",
    "react-keyed-flatten-children": "^1.3.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.0",
    "tastycss": "^0.8.2",
    "tiny-invariant": "^1.2.0"
  },
  "homepage": "https://github.com/OutpostHQ/jenga-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OutpostHQ/jenga-ui.git"
  },
  "bugs": {
    "url": "https://github.com/OutpostHQ/jenga-ui/issues"
  }
