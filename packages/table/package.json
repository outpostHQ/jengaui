{
  "name": "@jenga-ui/table",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Jenga-UI React table component",
  "keywords": [
    "table",
    "react",
    "jenga-ui",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jenga-ui/checkbox": "workspace:packages/checkbox",
    "@jenga-ui/content": "workspace:packages/content",
    "@jenga-ui/core": "workspace:packages/core",
    "@jenga-ui/layout": "workspace:packages/layout",
    "@jenga-ui/providers": "workspace:packages/providers",
    "@jenga-ui/utils": "workspace:packages/utils",
    "@react-aria/table": "^3.4.1",
    "@react-aria/visually-hidden": "^3.3.1",
    "@react-stately/data": "^3.6.1",
    "@react-stately/table": "^3.4.0",
    "@react-types/grid": "^3.1.3",
    "@react-types/shared": "^3.10.1",
    "tastycss": "^0.8.6"
  },
  "homepage": "https://github.com/OutpostHQ/jenga-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OutpostHQ/jenga-ui.git"
  },
  "bugs": {
    "url": "https://github.com/OutpostHQ/jenga-ui/issues"
  }
}
