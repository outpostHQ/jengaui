{
  "name": "@jengaui/button",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Jenga UI Button component in React",
  "keywords": [
    "button",
    "react",
    "Jenga UI",
    "ui",
    "clickable",
    "tabbable"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "*",
    "@react-types/button": "^3.4.1",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/form": "^0.1.4",
    "@jengaui/providers": "^0.1.4",
    "@jengaui/utils": "^0.1.4",
    "@react-aria/button": "^3.3.4",
    "@react-aria/interactions": "^3.7.0",
    "@react-spectrum/utils": "^3.6.3",
    "@react-types/shared": "^3.10.1",
    "tastycss": "^0.8.8"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
