{
  "name": "@jengaui/alert-dialog",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Jenga UI Alert Dialog component in React",
  "keywords": [
    "alert-dialog",
    "alert dialog",
    "alertdialog",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/button": "^0.1.4",
    "@jengaui/button-group": "^0.1.4",
    "@jengaui/content": "^0.1.4",
    "@jengaui/dialog": "^0.1.4",
    "@jengaui/portal": "^0.1.4",
    "@react-aria/utils": "^3.11.0",
    "@react-types/dialog": "^3.3.4",
    "tastycss": "^0.8.8",
    "tiny-invariant": "^1.2.0"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
