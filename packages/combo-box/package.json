{
  "name": "@jengaui/combo-box",
  "version": "0.1.3",
  "license": "MIT",
  "description": "Jenga UI Combo-Box component in React",
  "keywords": [
    "combo-box",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --minify --treeshake --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/form": "^0.1.3",
    "@jengaui/layout": "^0.1.3",
    "@jengaui/overlays": "^0.1.3",
    "@jengaui/providers": "^0.1.3",
    "@jengaui/select": "^0.1.3",
    "@jengaui/text-input": "^0.1.3",
    "@jengaui/utils": "^0.1.3",
    "@react-aria/button": "^3.3.4",
    "@react-aria/combobox": "^3.2.1",
    "@react-aria/i18n": "^3.3.4",
    "@react-aria/interactions": "^3.7.0",
    "@react-aria/overlays": "^3.7.5",
    "@react-aria/utils": "^3.11.0",
    "@react-stately/collections": "^3.3.4",
    "@react-stately/combobox": "^3.0.1",
    "@react-types/combobox": "^3.2.1",
    "@react-types/shared": "^3.10.1",
    "tastycss": "^0.8.8"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  }
}
