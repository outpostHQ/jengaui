{
  "name": "@jengaui/copy-snippet",
  "version": "0.1.2",
  "license": "MIT",
  "description": "Jenga UI Copy-Snippet component in React",
  "keywords": [
    "copy-snippet",
    "copy snippet",
    "copysnippet",
    "react",
    "Jenga UI",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --minify --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --minify --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "*",
    "@jengaui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jengaui/button": "^0.1.2",
    "@jengaui/card": "^0.1.2",
    "@jengaui/notification": "^0.1.2",
    "@jengaui/prism-code": "^0.1.2",
    "@jengaui/toast": "^0.1.2",
    "@jengaui/tooltip": "^0.1.2",
    "clipboard-copy": "^4.0.1",
    "tastycss": "^0.8.8"
  },
  "homepage": "https://github.com/Outposthq/jengaui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outposthq/jengaui.git"
  },
  "bugs": {
    "url": "https://github.com/Outposthq/jengaui/issues"
  },
  "resolutions": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11"
  }
}
