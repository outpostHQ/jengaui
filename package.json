{
  "name": "jengaui",
  "private": true,
  "scripts": {
    "build": "JSX=1 tsup --minify --treeshake",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test-watch": "jest --watch",
    "dev": "turbo dev --no-cache --parallel --continue",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,svg}\"",
    "publish:all": "lerna publish --no-verify-access --no-private",
    "publish:beta": "lerna publish --ignore-prepublish --ignore-scripts --canary --preid beta --dist-tag beta",
    "publish:dev": "run-s build:dev publish:beta"
  },
  "dependencies": {
    "@types/react-transition-group": "^4.4.2",
    "find-up": "^6.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "tastycss": "^0.8.8"
  },
  "devDependencies": {
    "@storybook/builder-vite": "^0.2.2",
    "@swc/core": "1.2.148",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-is": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "bytes": "^3.1.0",
    "chromatic": "^6.8.0",
    "dedent": "^0.7.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": ">=6",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "lerna": "^5.4.3",
    "lint-staged": ">=10",
    "node-abort-controller": "^3.0.1",
    "prettier": "^2.5.1",
    "react-router": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.3.0",
    "tsup": "^6.2.3",
    "turbo": "^1.5.3",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "workspaces": [
    "packages/*",
    "next-app",
    "examples",
    "storybook"
  ],
  "resolutions": {
    "es5-ext": "0.10.53",
    "prettier": "2.5.1",
    "@testing-library/user-event": "14.2.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari major versions"
  ],
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*": "prettier --write"
  },
  "packageManager": "yarn@3.0.2"
}
